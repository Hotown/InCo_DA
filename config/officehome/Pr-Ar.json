{
    "seed": 42,
    "exp_base": "./exps",
    "exp_name": "officehome",
    "exp_id": "Product-Art",
    "agent": "FixDANNAgent",
    "data_params": {
        "name": "office_home",
        "source": "Product",
        "target": "Art",
        "aug": "aug_0"
    },
    "num_epochs": 100,
    "warm_up": 10,
    "steps_epoch": null,
    "optim_params": {
      "learning_rate": 0.01,
      "conv_lr_ratio": 0.1,
      "patience": 8,
      "batch_size": 64,
      "decay": true,
      "weight_decay": 5e-4,
      "cls_update": true
    },
    "model_params": {
      "mlp": true,
      "mix": true,
      "out_dim": 512,
      "version": "pretrain-resnet50",
      "load_weight": "src-tgt",
      "load_weight_thres": 10,
      "load_weight_epoch": 5
    },
    "loss_params": {
      "thres_src": 0.99,
      "thres_tgt": 0.99,
      "temp": 0.1,
      "loss": [
        "cls-so",
        "proto-each",
        "I2M-cross",
        "tgt-condentmax",
        "tgt-entmin"
      ],
      "weight": [1, 1, 1, 0, 0.05],
      "clus": {
        "kmeans_freq": 1,
        "type": ["each"],
        "n_k": 15,
        "k": [65, 130]
      }
    }
}